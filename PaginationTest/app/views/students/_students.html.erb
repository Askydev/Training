<% @students.each do |student| %>
	<tr>
		<td>
			<%= student.name %>
		</td>
		<td>
			<%= student.subject %>
		</td>
		<td>
			<%= student.rollno %>
		</td>
		<td>
			<%= link_to 'Show', student_path(student) %>
		</td>
		<td>
			<%= link_to 'Edit', edit_student_path(student)%>
		</td>
		<td>
			<%= link_to 'Delete', student_path(student),method: :delete, data: {confirm: 'Are you sure you want to delete student'}%>
		</td>
	</tr>
<% end %>

