<%= form_with model: @address, url: "" ,:html => {:id => "add_address"} do|f| %>
<%= f.label :country%>
<%= f.text_field :country %><br>
<%= f.label :state%>
<%= f.text_field :state %><br>
<%= f.label :city%>
<%= f.text_field :city %><br>
<%= f.label :locality%>
<%= f.text_field :locality %><br>
<%= f.label :street%>
<%= f.text_field :street %><br>
<%= f.label :house_number%>
<%= f.text_field :house_no %><br>
<%= f.label :zip_code%>
<%= f.text_field :zip_code %><br>

<%= f.submit "Submit"%>
<% end %>
